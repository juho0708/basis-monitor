{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "/static/$1"
    }
  ],
  "functions": {
    "api/index.py": {
      "includeFiles": "**"
    }
  },
  "rewrites": [
    {
      "source": "/",
      "destination": "/static/index.html"
    }
  ]
}
